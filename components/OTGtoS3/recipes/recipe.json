{
  "RecipeFormatVersion": "2020-01-25",
  "ComponentName": "OTGtoS3",
  "ComponentVersion": "1.0.x",
  "ComponentDescription": "Send OTG Files to S3 after Processing.",
  "ComponentPublisher": "WirelessEco",
  "ComponentConfiguration": {},
  "Manifests": [
    {
      "Platform": {
        "os": "linux"
      },
      "Lifecycle": {
        "run": "node {artifacts:decompressedPath}/OTGtoS3/OTGtoS3/artifacts/OTGtoS3.js"
      },
      "Configuration": {
        "DefaultConfiguration": {
          "uploadBucketName": "{uploadBucketName}"
        }
      },
      "Artifacts": [
        {
          "URI": "s3://{componentsBucketName}/OTGtoS3/{componentVersion}/OTGtoS3.zip",
          "Unarchive": "ZIP"
        }
      ]
    },
    {
      "Platform": {
        "os": "windows"
      },
      "Lifecycle": {
        "run": "node {artifacts:decompressedPath}\\OTGtoS3\\OTGtoS3\\artifacts\\OTGtoS3.js "
      },
      "Configuration": {
        "DefaultConfiguration": {
          "uploadBucketName": "{uploadBucketName}"
        }
      },
      "Artifacts": [
        {
          "URI": "s3://{componentsBucketName}/OTGtoS3/{componentVersion}/OTGtoS3.zip",
          "Unarchive": "ZIP"
        }
      ]
    }
  ]
}
